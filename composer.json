{
    "name": "fastd/fastd-module",
    "type": "project",
    "keywords": [
        "module develop"
    ],
    "description": "The fastd module framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "janhuang",
            "email": "bboyjanhuang@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "fastd/fastd": "dev-dev"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Module\\": "Module"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "post-install-cmd": [
            "Module\\Module::init",
            "phpunit"
        ],
        "post-update-cmd": [
            "Module\\Module::init",
            "phpunit"
        ],
        "post-create-project-cmd": [
            "Module\\Module::init",
            "phpunit"
        ]
    }
}
